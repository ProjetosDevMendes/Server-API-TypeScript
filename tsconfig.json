{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  //Arquvios que vamos trabalhar com o TS
  "include": ["src/**/*"],
  "exclude": ["node_modules/", "dist"]
}
